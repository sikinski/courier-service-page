.start 
    display flex
    position relative
    padding 0 35px
    width 100%
    height 624px
    .navigation
        display flex
        position relative
        flex-direction column
        justify-content space-between
        width 21%
        height 100%
        .logo 
            margin 37px 0
            font-family 'Nova Script'
            font 'NovaScript'
            font-size 23px
            line-height 28px
            text-transform uppercase
        .nav_panel 
            position relative
            width 100%
            .nav_item
                width 100%
                padding 8px 0
                a 
                    font-weight 500
                    font-size 15px
                    line-height 42px
                    text-transform uppercase
                    font-family 'Poppins'
                    font 'Poppins'
        .social-networks_btns 
            display flex
            position relative
            width 100%
            .social-net_btn
                width 40px
                height 40px
                margin-right: 19px
                border-radius 50%
                box-shadow -5px 9px 8px rgba(0, 0, 0, 0.18)
                .social_icon
                    margin 0 auto
            .facebook_btn
                background linear-gradient(135deg, #C8DCFF 0%, #004ACB 98.16%)
            .google_btn
                background: linear-gradient(144.44deg, #FFB6B6 0%, #F62B19 116.39%);
            .twitter_btn
                background: linear-gradient(138.9deg, #FFFFFF 0%, #00B99E 120.57%);